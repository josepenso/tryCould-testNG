<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Cross browser Testing" parallel="tests"  thread-count="2" data-provider-thread-count="1">




    <test name="Chrome" >
        <parameter name="Browser" value="chrome"></parameter>
        <classes>
            <class name="com.trycloud.runners.CukesRunner"></class>
        </classes>
    </test>
    
     <test name="firefox">
        <parameter name="Browser" value="firefox"></parameter>
        <classes>
            <class name="com.trycloud.runners.CukesRunner2"></class>
        </classes>
    </test>























</suite>
